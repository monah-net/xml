                                        <function_value function="=uuid()"/>
                                    </value>
                                </set_model_property>
                                <refresh_component component_id="Customer_Details"/>
                            </action_chain>
                            <composite_enable_condition/>
                            <composite_visibility_condition/>
                        </button>
