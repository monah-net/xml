      id="fcrs_customer_info_snippet">
    <context>
        <variable key="project" value="FATCA_CIC"/>
        <variable key="branch" value="2019_v03"/>
        <variable key="filePath" value="C:\\axiom\\server\\aws_cv_fatca\\temp\\"/>
