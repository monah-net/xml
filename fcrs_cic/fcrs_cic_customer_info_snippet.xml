                                    <function_value function="=uuid()"/>
                                </value>
                            </set_model_property>
                            <composite_enable_condition/>
                            <composite_visibility_condition/>
                        </button>
                        <button text="Submit" style="primary-button">
                            <action_chain>
                                <data_source_update>
                                    <source_address project="${project}" branch="${branch}" name="dbd_cic_1_4_customer_details"/>
                                    <fields>
                                        <field name="rm_name">
                                            <model_value reference="rm_list_assign_tasks"/>
                                        </field>
                                    </fields>
                                    <keys>
                                        <field name="customer_number">
                                            <model_value reference="Customer_Details"/>
                                        </field>
                                    </keys>
                                    <instance>
                                        <instance_selector_ref reference="cic_instance_selector"/>
                                    </instance>
                                </data_source_update>
                                <data_source_update>
                                    <source_address project="${project}" branch="${branch}" name="dbd_task_details"/>
                                    <fields>
                                        <field name="time_stamp">
                                            <function_value function="=currentDateInMillis()"/>
                                        </field>
                                        <field name="task_number">
                                            <function_value function="=uuid()"/>
                                        </field>
                                    </fields>
                                    <keys>
                                        <field name="customer_number">
                                            <model_value reference="Customer_Details"/>
                                        </field>
                                        <field name="rm_name">
                                            <model_value reference="rm_list_assign_tasks"/>
                                        </field>
                                        <field name="task_name">
                                            <model_value reference="task_name_list"/>
                                        </field>
                                    </keys>
                                    <instance>
                                        <instance_selector_ref reference="cic_instance_selector"/>
                                    </instance>
                                </data_source_update>
                                <data_source_update>
                                    <source_address project="${project}" branch="${branch}" name="cic_history_source"/>
                                    <fields>
                                        <field name="action_name">
                                            <string_value value="Assign Task"/>
                                        </field>
                                        <field name="comment_val">
                                            <model_value reference="comment_task"/>
                                        </field>
                                        <field name="user_name">
                                            <function_value function="=userName()"/>
                                        </field>
                                        <field name="user_ID">
                                            <function_value function="=userId()"/>
                                        </field>
                                    </fields>
                                    <keys>
