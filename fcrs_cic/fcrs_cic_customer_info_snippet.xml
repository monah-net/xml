    <h_section id="section_instance_picker_cic_summary" height="50px" width="100%" align="TOP_LEFT">
        <separate_instance_selector id="cic_instance_selector"
                                    width="60%" use_description_for_name="true">
            <data_source project="${project}" branch="${branch}" source="dbd_cic_1_4_customer_details"
                         type="DataSource"/>
        </separate_instance_selector>
    </h_section>
    <spacer width="20px" height="30px"/>
    <h_section>
        <dropdown id="Customer_Details">
            <data_source project="${project}" branch="${branch}" source="dbd_cic_1_4_customer_details" type="DataSource"/>
            <value field="customer_number"/>
            <instance>
                <instance_selector_ref reference="cic_instance_selector"/>
            </instance>
            <composite_read_only_condition/>
        </dropdown>
        </h_section>
    <h_section>
        <grid id="CustDetails">
            <data_source project="${project}" branch="${branch}" source="dbd_cic_1_4_customer_details"
                         type="DataSource"/>
            <instance>
                <instance_selector_ref reference="Customer_Details"/>
            </instance>
            <filter empty_value_policy="ALL">
                <filter_value>
                    <model_value reference="Customer_Details"/>
                </filter_value>
                <filter_mapping operator="EQUALS">
                    <filter_on name="customer_number"/>
                </filter_mapping>
            </filter>
            <column id="cd1" title="Customer Number" alignment="LEFT" display="true" is_filtered="false" width="98%">
                <component>
                    <grid_label_component alignment="LEFT">
                        <dynamic_caption>
                            <field_value field="customer_number"/>
                        </dynamic_caption>
                    </grid_label_component>
                </component>
            </column>
            <column id="cd2" title="First Name" alignment="LEFT" display="true" is_filtered="false" width="98%">
                <component>
                    <grid_label_component>
                        <dynamic_caption>
                            <field_value field="first_name"/>
                        </dynamic_caption>
                    </grid_label_component>
                </component>
            </column>
            <column id="cd3" title="Middle Name" alignment="LEFT" display="true" is_filtered="false" width="98%">
                <component>
                    <grid_label_component>
                        <dynamic_caption>
                            <field_value field="middle_name"/>
                        </dynamic_caption>
                    </grid_label_component>
                </component>
            </column>
            <column id="cd4" title="Last Name" alignment="LEFT" display="true" is_filtered="false" width="98%">
                <component>
                    <grid_label_component>
                        <dynamic_caption>
                            <field_value field="last_name"/>
                        </dynamic_caption>
                    </grid_label_component>
                </component>
            </column>
            <column id="cd5" title="Last Name" alignment="LEFT" display="true" is_filtered="false" width="98%">
                <component>
                    <grid_label_component>
                        <dynamic_caption>
                            <field_value field="last_name"/>
                        </dynamic_caption>
                    </grid_label_component>
                </component>
            </column>
            <column id="cd6" title="Business Name" alignment="LEFT" display="true" is_filtered="false" width="98%">
                <component>
                    <grid_label_component>
                        <dynamic_caption>
                            <field_value field="business_name"/>
                        </dynamic_caption>
                    </grid_label_component>
                </component>
            </column>
        </grid>
    </h_section>
