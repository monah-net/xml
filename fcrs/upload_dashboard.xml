<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="dashboardsConfig.xsd">
    <context>
        <variable key="project" value="FCRS_UI"/>
        <variable key="branch" value="current"/>
        <variable key="filePath" value="/opt/axiom/filefolder/"/>
        <variable key="wf_data_load" value="MAIN_DATA_LOAD_UI"/>
    </context>
<!--    <h_section id="section_instance_picker_upload" height="70px" width="100%" align="TOP_LEFT">-->
<!--        <separate_instance_selector id="upload_selectr" width="60%" use_description_for_name="true">-->
<!--            <data_source project="${project}" branch="${branch}" source="UI:dbd_signoff_report" type="DataModel"/>-->
<!--        </separate_instance_selector>-->
<!--    </h_section>-->
    <h_section style="heading-1" width="100%" height="70px">
            <static_label id="upload_header" text="Upload your template" icon_alignment="BOTTOM"/>
    </h_section>
    <grid id="grid1" style="data_load_grid">
        <data_source project="FILE_UPLOADER" branch="FILE_UPLOADER" source="customers" type="DataSource"/>
        <column id="FolderName" title="FolderName" display="false">
            <component>
                <grid_label_component>
                    <static_caption text="FCRS_DATA_DIR"/>
                </grid_label_component>
            </component>
        </column>
        <column id="dm_upload" title="DM Upload">
            <component>
                <grid_dm_upload_component file_folder="FCRS_DATA_DIR">
                    <static_caption icon="upload.svg"/>
                    <unique_id>
                        <field_value field="customer_number"/>
                    </unique_id>
                    <source_address project="FILE_UPLOADER" branch="FILE_UPLOADER"
                                    name="sys_document_managment_history"/>
                    <composite_enable_condition/>
                </grid_dm_upload_component>
            </component>
        </column>
        <column id="dm_download" title="DM Download">
            <component>
                <grid_button_component>
                    <static_caption icon="download.svg"/>
                    <dm_download file_folder="FCRS_DATA_DIR">
                        <unique_id>
                            <field_value field="customer_number"/>
                        </unique_id>
                        <source_address project="FILE_UPLOADER" branch="FILE_UPLOADER"
                                        name="sys_document_managment_history"/>
                    </dm_download>
                    <composite_enable_condition/>
                </grid_button_component>
            </component>
        </column>
    </grid>
    <h_section width="100%" height="100%">
        <run_workflow_button id="wf_run" status_path="wf_status" text="RUN" style="solid"
                             icon_alignment="RIGHT">
            <icon status_value="ACTIVE" icon_value="play.svg"/>
            <icon status_value="NONE" icon_value="play.svg"/>
            <icon status_value="START" icon_value="spinner.svg"/>
            <icon status_value="WAITING" icon_value="spinner.svg"/>
            <icon status_value="KILLED" icon_value="play.svg"/>
            <icon status_value="ERROR" icon_value="play.svg"/>
            <icon status_value="WARNING" icon_value="play.svg"/>
            <workflow_action restart_all_modules="true" status_path="wf_status">
                <workflow>
                    <project>
                        <model_value reference="{$project}"/>
                    </project>
                    <branch>
                        <model_value reference="{$branch}"/>
                    </branch>
                    <name>
                        <string_value value="{$wf_data_load}"/>
                    </name>
                </workflow>
                <instance>
                    <instance_provider>
                        <as_of_date>
                            <model_value reference="as_of_date_load"/>
                        </as_of_date>
                        <keys>
                            <variable name="">
                                <model_value reference=""/>
                            </variable>
                            <variable name="">
                                <model_value reference=""/>
                            </variable>
                            <variable name="">
                                <model_value reference=""/>
                            </variable>
                        </keys>
                    </instance_provider>
                    <instance_selector_ref reference="upload_selectr"/>
                </instance>
                <variables>
                    <variable name="">
                        <model_value reference=""/>
                    </variable>
                </variables>
            </workflow_action>
            <composite_enable_condition/>
            <composite_visibility_condition/>
        </run_workflow_button>
        <text_area id="filing_type" caption="Filing Type">
            <composite_enable_condition/>
        </text_area>
        <text_area id="acct_info_type" caption="Aggregated (AGGREGATED) / D etailed (NULL)">
            <composite_enable_condition/>
        </text_area>
        <text_area id="file_name" caption="Input File Name">
            <composite_enable_condition/>
        </text_area>
        <text_area id="pool_report_source" caption="FATCA (F)/ CRS (C)">
            <composite_enable_condition/>
        </text_area>
        <text_area id="fatca_crs" caption="FATCA (F)/ CRS (C)">
            <composite_enable_condition/>
        </text_area>
        <text_area id="fatca_crs" caption="FATCA (F)/ CRS (C)">
            <composite_enable_condition/>
        </text_area>
    </h_section>
</page>